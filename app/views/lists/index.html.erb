<h1>Lists index</h1>
<h1>User lists</h1>
<% @lists.each do |list| %>
<ul>
  <%= list.id %>
  <%= list.title %>
  <%= list.user.email %>
  <%= link_to "Show entries", list_list_entries_path(list), method: :get %>
  <%# <%= link_to "Display list", list_path(list), method: :get %>
  <%= link_to "Delete list", list_path(list), method: :delete %>
</ul>
<% end %>

<h1>All lists</h1>
<% @all_lists.each do |list| %>
<ul>
  <%= list.id %>
  <%= list.title %>
  <%= list.user.email %>
  <%= link_to "Show entries", list_list_entries_path(list), method: :get %>
  <%# <%= link_to "Display list", list_path(list), method: :get %>
</ul>
<% end %>